{
  "name": "@fusorjs/dom",
  "version": "2.0.0",
  "license": "ISC",
  "homepage": "https://github.com/fusorjs/dom#readme",
  "author": "Igor Sukharev <imixaly4@gmail.com>",
  "description": "Fusor is a simple Javascript library that helps declaratively create and update DOM elements.",
  "keywords": [
    "dom",
    "fusor",
    "fusorjs",
    "@fusorjs",
    "ui",
    "interface",
    "frontend",
    "library"
  ],
  "scripts": {
    "test": "jest src",
    "testc": "jest src --coverage",
    "prettier": "prettier --write .",
    "start": "shx rm -rf dist/*  &&  NODE_ENV=development  tsc -p tsconfig.dist.json --watch",
    "build": "shx rm -rf dist/*  &&  NODE_ENV=production  tsc -p tsconfig.dist.json",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./svg": "./dist/help/svg.js",
    "./html": "./dist/help/html.js",
    "./life": "./dist/help/life.js",
    "./help": "./dist/help/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "svg": [
        "dist/help/svg.d.ts"
      ],
      "html": [
        "dist/help/html.d.ts"
      ],
      "life": [
        "dist/help/life.d.ts"
      ],
      "help": [
        "dist/help/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
