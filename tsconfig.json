{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "declaration": true,
    "skipLibCheck": true,
    "strict": true,
    "isolatedModules": true,
    "jsx": "react",
    "jsxFactory": "jsx"
  },
  "include": ["src"]
}
